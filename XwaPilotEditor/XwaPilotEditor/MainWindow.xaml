                                                      Tag="{Binding CraftNames, ElementName=main}" />
                                <TextBlock Text="KillsAssistsPerCraftPerMissionType:" />
                                <local:DataGridEditor ItemsSource="{Binding Pilot.KillsAssistsPerCraftPerMissionType, Converter={x:Static local:ArrayBinderConverter.Default}, ConverterParameter={x:Type pilot:PilotMissionCategory}}"
                                                      Tag="{Binding CraftNames, ElementName=main}" />
