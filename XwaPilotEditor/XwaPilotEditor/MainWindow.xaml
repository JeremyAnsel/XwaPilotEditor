                                <TextBlock Text="KillsSharedFromPlayer:" />
                                <local:ListBoxEditor ItemsSource="{Binding Pilot.KillsSharedFromPlayer}" />
                            </StackPanel>
                        </ScrollViewer>
                    </TabItem>
                    <TabItem Header="KillsPerFlightGroup">
                        <ScrollViewer>
                            <StackPanel>
                                <TextBlock Text="KillsFullOnFlightGroup:" />
                                <local:ListBoxEditor ItemsSource="{Binding Pilot.KillsFullOnFlightGroup}" />
                                <TextBlock Text="KillsSharedOnFlightGroup:" />
                                <local:ListBoxEditor ItemsSource="{Binding Pilot.KillsSharedOnFlightGroup}" />
                                <TextBlock Text="KillsFullFromFlightGroup:" />
                                <local:ListBoxEditor ItemsSource="{Binding Pilot.KillsFullFromFlightGroup}" />
                                <TextBlock Text="KillsSharedFromFlightGroup:" />
                                <local:ListBoxEditor ItemsSource="{Binding Pilot.KillsSharedFromFlightGroup}" />
                            </StackPanel>
                        </ScrollViewer>
                    </TabItem>
                    <TabItem Header="FlightGroupRating">
                        <ScrollViewer>
                            <StackPanel>
                                <TextBlock Text="FlightGroupRating:" />
                                <local:ListBoxEditor ItemsSource="{Binding Pilot.FlightGroupRating}" />
                            </StackPanel>
                        </ScrollViewer>
                    </TabItem>
                </TabControl>
            </TabItem>
            <TabItem Header="ObjectStats">
                <TabControl>
                    <TabItem Header="PerMissionType">
                        <ScrollViewer>
                            <StackPanel>
                                <TextBlock Text="TotalScorePerMissionType:" />
                                <local:ListBoxEditor ItemsSource="{Binding Pilot.ObjectStats.TotalScorePerMissionType}"
                                                     Tag="{x:Type pilot:PilotMissionCategory}" />
                                <TextBlock Text="TotalMissionsPlayedCountPerMissionType:" />
                                <local:ListBoxEditor ItemsSource="{Binding Pilot.ObjectStats.TotalMissionsPlayedCountPerMissionType}"
                                                     Tag="{x:Type pilot:PilotMissionCategory}" />
                                <TextBlock Text="NumOfSpecialCraftInspectedPerMissionType:" />
                                <local:ListBoxEditor ItemsSource="{Binding Pilot.ObjectStats.NumOfSpecialCraftInspectedPerMissionType}"
                                                     Tag="{x:Type pilot:PilotMissionCategory}" />
                                <TextBlock Text="EnergyWeaponsHitsPerMissionType:" />
                                <local:ListBoxEditor ItemsSource="{Binding Pilot.ObjectStats.EnergyWeaponsHitsPerMissionType}"
                                                     Tag="{x:Type pilot:PilotMissionCategory}" />
                                <TextBlock Text="EnergyWeaponsFiredPerMissionType:" />
