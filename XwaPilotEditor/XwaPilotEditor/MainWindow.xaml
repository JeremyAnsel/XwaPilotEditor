                                <local:DataGridEditor ItemsSource="{Binding Pilot.KillsAssistOnPlayerRatingPerMissionType, Converter={x:Static local:ArrayBinderConverter.Default}, ConverterParameter={x:Type pilot:PilotMissionCategory}}"
                                                      Tag="{x:Type pilot:PilotRating}" />
                            </StackPanel>
