<Window x:Class="XwaPilotEditor.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:XwaPilotEditor"
        xmlns:pilot="clr-namespace:JeremyAnsel.Xwa.Pilot;assembly=JeremyAnsel.Xwa.Pilot"
        mc:Ignorable="d"
        d:DataContext="{d:DesignInstance local:MainWindow}"
        Title="{Binding PilotFileName, StringFormat='Xwa Pilot Editor {0}', FallbackValue='Xwa Pilot Editor'}"
        Height="500"
        Width="850"
        UseLayoutRounding="True"
        x:Name="main">
    <Window.CommandBindings>
        <CommandBinding Command="ApplicationCommands.New"
                        Executed="ExecuteNewPilot" />
        <CommandBinding Command="ApplicationCommands.Open"
                        Executed="ExecuteOpenPilot" />
        <CommandBinding Command="ApplicationCommands.Save"
                        Executed="ExecuteSavePilot" />
        <CommandBinding Command="ApplicationCommands.SaveAs"
                        Executed="ExecuteSaveAsPilot" />
    </Window.CommandBindings>
    <DockPanel>
        <TextBlock DockPanel.Dock="Bottom"
                   Text="{Binding PilotFileName}" />
        <ToolBar DockPanel.Dock="Top"
                 ToolBarTray.IsLocked="True">
            <Button Command="ApplicationCommands.New">New</Button>
            <Button Command="ApplicationCommands.Open">Open...</Button>
            <Button Command="ApplicationCommands.Save">Save</Button>
            <Button Command="ApplicationCommands.SaveAs">Save As...</Button>
        </ToolBar>
        <TabControl>
            <TabItem Header="General">
                <ScrollViewer>
                    <StackPanel>
                        <TextBlock Text="Name:" />
                        <TextBox Text="{Binding Pilot.Name, UpdateSourceTrigger=PropertyChanged}" />
                        <TextBlock Text="TotalScore:" />
                        <TextBox Text="{Binding Pilot.TotalScore, UpdateSourceTrigger=PropertyChanged}" />
                        <TextBlock Text="NumHumanPlayersLastMission:" />
                        <TextBox Text="{Binding Pilot.NumHumanPlayersLastMission, UpdateSourceTrigger=PropertyChanged}" />
                        <TextBlock Text="M00022_ABD7B4:" />
                        <TextBox Text="{Binding Pilot.M00022_ABD7B4, UpdateSourceTrigger=PropertyChanged}" />
                        <TextBlock Text="Team:" />
                        <TextBox Text="{Binding Pilot.Team, UpdateSourceTrigger=PropertyChanged}" />
                        <TextBlock Text="MissionDirectoryId:" />
                        <local:EnumComboBox EnumType="{x:Type pilot:PilotMissionDirectory}"
                                            Text="{Binding Pilot.MissionDirectoryId}" />
                        <TextBlock Text="MissionDescriptionIds:" />
                        <local:ListBoxEditor ItemsSource="{Binding Pilot.MissionDescriptionIds}"
                                             Tag="{x:Type pilot:PilotMissionDirectory}" />
                        <TextBlock Text="MultiplayerGameName:" />
                        <TextBox Text="{Binding Pilot.MultiplayerGameName, UpdateSourceTrigger=PropertyChanged}" />
                        <TextBlock Text="MultiplayerHostName:" />
                        <TextBox Text="{Binding Pilot.MultiplayerHostName, UpdateSourceTrigger=PropertyChanged}" />
                        <TextBlock Text="CurrentRatingPromoPoints:" />
                        <TextBox Text="{Binding Pilot.CurrentRatingPromoPoints, UpdateSourceTrigger=PropertyChanged}" />
                        <TextBlock Text="CurrentRatingWorsePromoPoints:" />
                        <TextBox Text="{Binding Pilot.CurrentRatingWorsePromoPoints, UpdateSourceTrigger=PropertyChanged}" />
                        <TextBlock Text="NewPromotion:" />
                        <TextBox Text="{Binding Pilot.NewPromotion, UpdateSourceTrigger=PropertyChanged}" />
                        <TextBlock Text="NextPromotionPercent:" />
                        <TextBox Text="{Binding Pilot.NextPromotionPercent, UpdateSourceTrigger=PropertyChanged}" />
                        <TextBlock Text="MissionScore:" />
                        <TextBox Text="{Binding Pilot.MissionScore, UpdateSourceTrigger=PropertyChanged}" />
                        <TextBlock Text="NewCraftAddedToTechRoom:" />
                        <TextBox Text="{Binding Pilot.NewCraftAddedToTechRoom, UpdateSourceTrigger=PropertyChanged}" />
                        <TextBlock Text="PilotRating:" />
                        <local:EnumComboBox EnumType="{x:Type pilot:PilotRating}"
                                            Text="{Binding Pilot.PilotRating}" />
                        <TextBlock Text="PilotRank:" />
                        <local:EnumComboBox EnumType="{x:Type pilot:PilotRank}"
                                            Text="{Binding Pilot.PilotRank}" />
                        <TextBlock Text="KalidorCresent:" />
                        <TextBox Text="{Binding Pilot.KalidorCresent, UpdateSourceTrigger=PropertyChanged}" />
                        <TextBlock Text="TotalMissionsPlayedCount:" />
                        <TextBox Text="{Binding Pilot.TotalMissionsPlayedCount, UpdateSourceTrigger=PropertyChanged}" />
                        <TextBlock Text="TotalMissionsPlayedCountPerRating:" />
                        <local:ListBoxEditor ItemsSource="{Binding Pilot.TotalMissionsPlayedCountPerRating}"
                                             Tag="{x:Type pilot:PilotRating}" />
                        <TextBlock Text="PilotRatingName:" />
                        <TextBox Text="{Binding Pilot.PilotRatingName, UpdateSourceTrigger=PropertyChanged}" />
                        <TextBlock Text="EmkayAnnounceNewAward:" />
                        <TextBox Text="{Binding Pilot.EmkayAnnounceNewAward, UpdateSourceTrigger=PropertyChanged}" />
                        <TextBlock Text="EmkayAnnounceNewRank:" />
                        <TextBox Text="{Binding Pilot.EmkayAnnounceNewRank, UpdateSourceTrigger=PropertyChanged}" />
                        <TextBlock Text="FamilyNewMedal:" />
                        <TextBox Text="{Binding Pilot.FamilyNewMedal, UpdateSourceTrigger=PropertyChanged}" />
                        <TextBlock Text="TacOfficerAnnounceNewRank:" />
                        <TextBox Text="{Binding Pilot.TacOfficerAnnounceNewRank, UpdateSourceTrigger=PropertyChanged}" />
                        <TextBlock Text="CurrentFactionId:" />
                        <local:EnumComboBox EnumType="{x:Type pilot:PilotFactionId}"
                                            Text="{Binding Pilot.CurrentFactionId}" />
                        <TextBlock Text="EmailsSortCriterion:" />
                        <TextBox Text="{Binding Pilot.EmailsSortCriterion, UpdateSourceTrigger=PropertyChanged}" />
                        <TextBlock Text="CampaignMode:" />
                        <TextBox Text="{Binding Pilot.CampaignMode, UpdateSourceTrigger=PropertyChanged}" />
                        <TextBlock Text="SkipMissionsRemaining:" />
                        <TextBox Text="{Binding Pilot.SkipMissionsRemaining, UpdateSourceTrigger=PropertyChanged}" />
                        <TextBlock Text="RegionsCount:" />
                        <TextBox Text="{Binding Pilot.RegionsCount, UpdateSourceTrigger=PropertyChanged}" />
                        <TextBlock Text="MissionFileName:" />
                        <TextBox Text="{Binding Pilot.MissionFileName, UpdateSourceTrigger=PropertyChanged}" />
                        <TextBlock Text="ProvingGroundsMissionPerPlayer:" />
                        <local:ListBoxEditor ItemsSource="{Binding Pilot.ProvingGroundsMissionPerPlayer}"
                                             Tag="" />
                        <TextBlock Text="MeleeMissionIndex:" />
                        <TextBox Text="{Binding Pilot.MeleeMissionIndex, UpdateSourceTrigger=PropertyChanged}" />
                        <TextBlock Text="HangarType:" />
                        <local:EnumComboBox EnumType="{x:Type pilot:PilotHangar}"
                                            Text="{Binding Pilot.HangarType}" />
                    </StackPanel>
                </ScrollViewer>
            </TabItem>
            <TabItem Header="CurrentStats">
